{%- assign chars = "1,2,3,4,5,6,7,8,9,0,a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z" | split: "," %}
{%  assign numbers = "1,2,3,4,5,6,7,8" | split: "," %}
{%- assign random =0 %}
{%- for char in numbers %}
{%- assign tchar = chars | sample %}
{%- assign random = random | append: tchar %}
{%- endfor %}{{ random }}